    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/typeorm": "^7.1.0",
    "apollo-server-express": "^2.25.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "graphql": "^15.5.0",
    "graphql-tools": "^4.0.8",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "nestjs-pino": "^0.6.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pino-pretty": "^3.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.22"
  },
  "devDependencies": {
